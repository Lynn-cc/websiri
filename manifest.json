{
  "name": "WebSiri",
  "version": "0.1",
  "manifest_version": 2,
  "description": "My name is web siri !",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "popup.html"
  },
  "content_scripts": [
      {
          "matches": ["*://*/*"],
              //"css": ["mystyles.css"],
              "js": ["jquery.min.js", "page.js"]
      }
  ],

  //  "options_page": "options.html",
  "background": {
      "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
